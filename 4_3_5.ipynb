from math import * 
x = int(input('Введите x: ')) 
y = int(input('Введите y:')) 
action = int(input('Введите действие (1 - +, 2 - -, 3 - *, 4 - /, 5 - e^(x=y), 6 - sin(x+y), 7 - cos(x+y), 8 - x**y):')) 
if action == 1: 
    print(x+y) 
elif action == 2: 
    print(x-y) 
elif action == 3: 
    print(x*y) 
elif action == 4: 
    if (y == 0): 
        print('На ноль делить нельзя!') 
    else: 
        print(x/y) 
elif action == 5: 
    print(e**(x+y)) 
elif action == 6: 
    print(sin(x+y)) 
elif action == 7: 
    print(cos(x+y)) 
elif action == 8: 
    print(x**y)
